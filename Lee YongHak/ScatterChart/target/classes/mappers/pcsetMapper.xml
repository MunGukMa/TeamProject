<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="one.two.three.dao.PCSetMapper">
	
	<select id="pcsetList" resultType="PCSetVO">
	
		select *
		from 
			pcset
		where
			fitc_id = #{fitc_id}
	
	</select>
	
</mapper>

